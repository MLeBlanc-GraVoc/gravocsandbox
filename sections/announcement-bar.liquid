{%- comment -%}theme-check-disable SchemaJsonFormat{%- endcomment -%}
{%- liquid
  assign announcements_size = 0
  assign announcements_to_show = ""
  assign announcements_first_color = ""

  for block in section.blocks
    assign show_this_announcement = false
    if block.settings.text != blank
      unless block.settings.announcement_pages == blank
        assign match = block.settings.announcement_pages.links | where: 'url', request.path
        if match != blank
          assign show_this_announcement = true
        endif
      endunless

      if block.settings.announcement_pages == blank or show_this_announcement
        if announcements_first_color == ""
          assign announcements_first_color = block.settings.color_scheme
        endif
        assign announcements_size = announcements_size | plus: 1
        assign announcements_to_show = announcements_to_show | append: "," | append: forloop.index | append: ","
      endif
    endif
  endfor
-%}

{%- if announcements_size > 0 -%}
<link rel="stylesheet" href="{{ 'component-slideshow.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-slider.css' | asset_url }}" media="print" onload="this.media='all'">

<div
  class="utility-bar color-{{ announcements_first_color }} gradient {{ section.settings.text_size }}{% if section.settings.show_line_separator and announcements_size > 0 %} utility-bar--bottom-border{% endif %}{% if section.settings.enable_country_selector or section.settings.enable_language_selector %} header-localization{% endif %}"
  style="--announcement-padding: {{ section.settings.text_padding }}rem;"
  {{ block.shopify_attributes }}
>
  <div class="page-width flex">
    <slideshow-component
      class="announcement-bar md:self-center"
      role="region"
      aria-roledescription="{{ 'sections.announcements.carousel' | t }}"
      aria-label="{{ 'sections.announcements.announcement_bar' | t }}"
    >
      <div class="announcement-bar-slider slider-buttons w-full">
        <div
          class="grid grid--1-col slider slider--everywhere"
          id="Slider-{{ section.id }}"
          aria-live="polite"
          aria-atomic="true"
          data-autoplay="{% if announcements_size > 1 %}true{% else %}false{% endif %}"
          data-speed="{{ section.settings.change_slides_speed }}"
          style="flex-wrap: inherit;"
        >
          {%- for block in section.blocks -%}
            {%- assign this_index = "," | append: forloop.index | append: "," -%}
            {%- if announcements_to_show contains this_index -%}
              <div
                class="slideshow__slide announcement-slideshow__slide slider__slide grid__item grid--1-col{% unless forloop.first %} hidden-during-load{% endunless %}"
                id="Slide-{{ section.id }}-{{ forloop.index }}"
                {{ block.shopify_attributes }}
                data-color-scheme="color-{{ block.settings.color_scheme }}"
                role="group"
                aria-roledescription="{{ 'sections.announcements.announcement' | t }}"
                aria-label="{{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}"
                tabindex="-1"
              >
                <div
                  class="announcement-bar__announcement announcement-bar__announcement-left"
                  role="region"
                  aria-label="{{ 'sections.header.announcement' | t }}"
                  {{ block.shopify_attributes }}
                >
                  {%- if block.settings.text != blank -%}
                    <div class="announcement-bar__message xs:text-center rte">
                      {% if block.settings.mobile_text != blank %}
                        <span class="hidden-xs">{{ block.settings.text }}</span>
                        <span class="visible-xs">{{ block.settings.mobile_text }}</span>
                      {% else %}
                        {{ block.settings.text }}
                      {% endif %}
                    </div>
                  {%- endif -%}
                </div>
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>
      </div>
    </slideshow-component>

    {%- if request.design_mode -%}
      <style>
        /* theme editor power preview fix */
        .announcement-bar-slider .slider__slide[aria-hidden='true'] {
          visibility: hidden;
        }
      </style>
    {%- endif -%}

    <div class="announcement__icons flex items-center gap-2">
      {%- if section.settings.link_1_label != blank -%}
        <a href="{{ section.settings.link_1_url }}" class="{{ section.settings.text_size }} link un-underlined-link flex items-center gap-05 whitespace-nowrap hidden-xs">
          {%- if section.settings.link_1_icon != blank -%}
            {%- render 'icon', icon: section.settings.link_1_icon, size: 18 -%}
          {%- endif -%}
          {{ section.settings.link_1_label }}
        </a>
      {%- endif -%}

      {%- if section.settings.link_2_label != blank -%}
        <a href="{{ section.settings.link_2_url }}" class="{{ section.settings.text_size }} link un-underlined-link flex items-center gap-05 whitespace-nowrap hidden-xs">
          {%- if section.settings.link_2_icon != blank -%}
            {%- render 'icon', icon: section.settings.link_2_icon, size: 18 -%}
          {%- endif -%}
          {{ section.settings.link_2_label }}
        </a>
      {%- endif -%}

      {%- if shop.customer_accounts_enabled and section.settings.enable_accounts -%}
        <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="{{ section.settings.text_size }} link un-underlined-link flex items-center gap-05 whitespace-nowrap hidden-xs">
          {%- render 'icon', icon: 'silhouette_circle', size: 18 -%}
          {%- liquid
            if customer
              echo 'customer.account_fallback' | t
            else
              echo 'customer.log_in' | t
            endif
          -%}
        </a>
      {%- endif -%}

      <div class="localization-wrapper small-hide medium-hide no-js-hidden{% if section.settings.text_padding <= 1 %} localization-wrapper--h-full{% endif %}">
        {%- liquid
          if section.settings.enable_country_selector and localization.available_countries.size > 1
            assign show_country_picker = true
          endif

          if section.settings.enable_language_selector and localization.available_languages.size > 1
            assign show_language_picker = true
          endif
        -%}
        {%- if show_country_picker or show_language_picker -%}
          <localization-form data-prevent-hide>
            {%- form 'localization', id: 'AnnouncmentLocalizationForm', class: 'localization-form' -%}
              <div>
                {%- render 'localization',
                    localPosition: 'AnnouncementLocalization',
                    show_country_picker: show_country_picker,
                    show_language_picker: show_language_picker,
                    show_country_name: false,
                    show_country_filter: section.settings.show_country_filter,
                    always_show_currency: true,
                    disclosure_class: 'disclosure--has-arrow',
                    font_size_class: section.settings.text_size,
                    abbreviate: false
                -%}
              </div>
            {%- endform -%}
          </localization-form>
        {%- endif -%}
      </div>
    </div>
  </div>
</div>
{%- endif -%}

